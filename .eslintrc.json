{
  "extends": "airbnb",
  "globals": {
    "__DEVELOPMENT__": true,
    "__BUILD__": true,
    "__VERSION__": true,
    "window": true,
    "navigator": true,
    "document": true
  },
  "rules": {
    "import/no-extraneous-dependencies": {
      "devDependencies": false
    },
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-underscore-dangle": "off",
    "no-unused-expressions": "off",
    "no-restricted-globals": "off",
    "no-bitwise": "off"
  }
}
